
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加问题分类</title>
</head>
<body>
    <div style="padding: 15px; overflow: hidden">
        <form id="RequestionTypeAddForm" method="post" action="">
            <table class="tableForm" width="100%">
                <tr>
                    <th>
                        类型名称：
                    </th>
                    <td>
                        <input type="text" name="txtTypeName" id="txtTypeName" class="easyui-validatebox textbox"
                               data-options="required:true,validType:'length[2,20]'" style="width:150px;height:22px;" />
                    </td>
                </tr>
                <tr>
                    <th>
                        排序：
                    </th>
                    <td>
                        <input type="number" name="txtSort" id="txtSort" value="1" class="easyui-numberspinner"
                               data-options="min:1,max:100,editable:false" style="width:70px;height:22px;" />
                    </td>
                </tr>
            </table>
        </form>
    </div>
</body>
</html>
